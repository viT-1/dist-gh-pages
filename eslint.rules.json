{
	"rules": {
		"brace-style": ["error", "stroustrup"],
		"comma-dangle": ["error", "always-multiline"],
		"indent": ["error", "tab"],
		"no-trailing-spaces": "error",
		"no-var": "error",
		"quotes": ["error", "single"],
		"semi": ["error", "always"],
		"spaced-comment": ["error", "always"],
		"import/no-default-export": "error",
		"import/no-unresolved": ["error", { "ignore": ["\\/dist-gh-pages\\/.*\\.js"] }]
	}
}